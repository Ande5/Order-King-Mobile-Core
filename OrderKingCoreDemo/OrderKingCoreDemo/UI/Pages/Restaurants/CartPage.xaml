<?xml version="1.0" encoding="utf-8" ?>
<pages:BasePage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:OrderKingCoreDemo.UI.Pages;assembly=OrderKingCoreDemo"
             xmlns:orderKingCoreDemo="clr-namespace:OrderKingCoreDemo;assembly=OrderKingCoreDemo"
             x:Class="OrderKingCoreDemo.UI.Pages.Restaurants.CartPage">
        <StackLayout>
			<Label Text="CartPage"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
	<pages:BasePage.ToolbarItems>
		<ToolbarItem Text="Close" Command="{Binding GoBackCommand}" CommandParameter="{x:Static orderKingCoreDemo:NavigationMode.Modal}"/>
	</pages:BasePage.ToolbarItems>
</pages:BasePage>